<Window x:Class="SudokuWPF.View.PickupSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PickupSet" Height="300" Width="310.15">
    <WrapPanel Margin="0,0,-8,87">
        <Label Content="Please enter set from 1 to 20 to play." HorizontalAlignment="Left" Margin="50,23,0,0" VerticalAlignment="Top"/>
        <TextBox Name="txtSet" Width="192" Margin="50,10,0,0"></TextBox>
        <Button Name="btnOk" Content="Ok" Width="75" Margin="130, 70,0,0" Click="btnOk_Click"/>
        <Button Content="Cancel" Width="75" Margin="210,-22,0,0" Click="btnCancel_Click"/>
    </WrapPanel>
</Window>
