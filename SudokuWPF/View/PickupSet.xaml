<Window x:Class="SudokuWPF.View.PickupSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PickupSet" Height="180" Width="310.15">
    <WrapPanel>
        <Label Content="Please Enter Set Number from 1 to 20 to play:" HorizontalAlignment="Left" Margin="15,23,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox Name="txtSet" Width="192" Margin="50,10,0,0" Height="25" FontWeight="Bold" FontSize="15"></TextBox>
        <Button x:Name="btnOk" Content="OK" Width="75" Margin="50, 20,0,0" Click="btnOk_Click"/>
        <Button Content="Cancel" Width="75" Margin="40, 20,0,0" Click="btnCancel_Click"/>
    </WrapPanel>
</Window>
