<Window x:Class="SudokuWPF.View.PickupLevel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PickupLevel" Height="210" Width="310">
    <Grid Name="GridPickupLevel" Margin="0,0,-8,-7">
        <RadioButton Content="Beginer" HorizontalAlignment="Left" Margin="51,35,0,0" GroupName="GrLevel" Tag="0" VerticalAlignment="Top"/>
        <RadioButton Content="Moderate" HorizontalAlignment="Left" Margin="51,65,0,0" GroupName="GrLevel" Tag="1" VerticalAlignment="Top"/>
        <RadioButton Content="Advance" HorizontalAlignment="Left" Margin="51,95,0,0" GroupName="GrLevel" Tag="2" VerticalAlignment="Top"/>
        <Label Content="Please select a Level" HorizontalAlignment="Left" Margin="17,5,0,0" VerticalAlignment="Top" FontWeight="Bold" RenderTransformOrigin="-0.453,0.538"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="133,141,0,0" VerticalAlignment="Top" Width="75" Click="btnClickOk"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="213,141,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
